FluPaginationItem[checked="false"] {
    color: black;
    border: 1px solid rgb(229, 229, 229);
    border-radius: 4px;
    border-bottom: 1px solid rgb(204, 204, 204);
    background-color: rgb(251, 251, 251);
    font-size: 15px;
}

FluPaginationItem[checked=true] {
    color: rgb(255, 255, 255);
    border: 1px solid rgb(20, 103, 166);
    border-radius: 4px;
    border-bottom: 1px solid rgb(0, 54, 95);
    background-color: rgb(0, 90, 158);
    font-size: 15px;
}

